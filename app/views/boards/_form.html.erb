<%= form_for(board) do |f| %>
  <%= render 'layouts/error_messages', :target => @board %>
    
  <%= f.label :title %><br>
  <%= f.text_field :title %><br>
  <%= f.label :description %><br>
  <%= f.text_field :description %><br>
  
  <%= f.hidden_field :owner_id, value: user.id %><br>
  <%= f.submit %>
<% end %>
